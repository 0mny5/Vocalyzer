<div class="container">
  <div class="container">
    <%= form_with model: [:users, @project] do |f| %>
      <%= render "shared/error_messages", object: f.object %>

      <div style="height: 10px;"></div>

      <div class="field my-3">
        <%= f.label :title %>
        <%= f.text_field :title, class: 'form-control' %>
      </div>

      <div id="songs">
        <%= f.fields_for :songs do |song| %>
          <%= render 'song_fields', f: song %>
        <% end %>
        <div id="add-song" class="links">
          <%= link_to_add_association t('defaults.add_url'), f, :songs, class: 'btn btn-link' %>
        </div>
      </div>

      <div class="row justify-content-center m-3">
        <%= f.submit class: 'btn btn-secondary' %>
      </div>
    <% end %>
  </div>

  <div style="height: 10px;"></div>
  <%= render 'search_field' %>
  <div class="container">
    <div id="search-field" class="m-3">
    </div>
  </div>
</div>
