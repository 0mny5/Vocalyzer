<div class="container">
  <%= search_form_for @q, url: admin_users_path do |f| %>
    <div class="row justify-content-center">
      <div class="col-3">
        <%= f.search_field :name_cont, class: 'form-control', placeholder: 'ユーザー名を入力してください' %>
      </div>
      <%= f.submit '検索' %>
    </div>
  <% end %>
</div>
<%= render @users %>
