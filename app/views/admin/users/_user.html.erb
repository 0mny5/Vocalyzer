<div class="container">
  <div class="row">
    <div class="col-2 my-3">
      <%= user.id %>
    </div>
    <div class="col-6 my-3">
      <%= link_to edit_admin_user_path(user.id) do %>
        <%= user.name %>
      <% end %>
    </div>
    <div class="col-2 my-3">
      <%= image_tag user.avatar, class: 'rounded-circle', size: '30x30' %>
    </div>
    <div class="col-2 my-3">
      <%= user.role %>
    </div>
  </div>
</div>
